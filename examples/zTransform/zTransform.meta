################################################################################
################################################################################
#
# zTransform.meta - example of a dggrid meta-file which performs address
#                   conversion with ZOrder indexes
#
################################################################################

# specify the operation
dggrid_operation TRANSFORM_POINTS

# specify the DGG
dggs_type ISEA3H
dggs_res_spec 9

# specify bin controls
input_file_name inputfiles/geo.txt
input_address_type GEO
#input_file_name inputfiles/zorder.txt
#input_address_type ZORDER
input_delimiter " "

# specify the output
output_file_name outputfiles/output.txt
output_address_type ZORDER
#output_address_type ZORDER_STRING
output_delimiter " "
